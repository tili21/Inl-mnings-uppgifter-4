using System;

namespace Test
{
    class Program
    {
        static void Main(string[] args)
        {
            int summa = 0;

            for (int i = 0; i < 5; i++)
            {
                Console.WriteLine("Skriv ett heltal:");
                int tal = int.Parse(Console.ReadLine());
                summa += tal;
            }

            Console.WriteLine("Summan Ã¤r: " + summa);
        }
    }
}
